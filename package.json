{
  "name": "gsy-component-library",
  "version": "0.1.8",
  "description": "reusable components from grid singularity",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-css": "node-sass -r ./lib/components/ -o ./build/components/",
    "build-js": "babel lib -d build",
    "build": "npm run build-css && npm run build-js",
    "lint-es": "eslint . --ext js --ext jsx",
    "lint-sass": "sass-lint -c .sass-lint.yml './lib/**/*.scss' -v -q",
    "storybook": "NODE_ENV=storybook start-storybook -p 9001 -c .storybook",
    "watch-css": "nodemon -e scss -x npm run build-css",
    "watch-js": "nodemon -e js -x npm run build-js",
    "start-watch": "npm-run-all --parallel watch-js watch-css",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gridsingularity/component-library.git"
  },
  "keywords": [
    "react",
    "components",
    "component",
    "library",
    "living",
    "design",
    "guide"
  ],
  "author": "kathrin@gridsingularity.com",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/gridsingularity/component-library/issues"
  },
  "homepage": "https://github.com/gridsingularity/component-library#readme",
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.0-alpha.1",
    "@storybook/addon-info": "^3.4.0-alpha.1",
    "@storybook/react": "^3.2.17",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "del": "^3.0.0",
    "eslint": "^4.12.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "eslint-watch": "^3.1.3",
    "jshint": "^2.9.5",
    "nodemon": "^1.14.11",
    "npm-run-all": "^4.1.2",
    "sass-lint": "^1.12.1"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "node-sass": "^4.7.2",
    "prop-types": "^15.6.0",
    "rc-input-number": "^4.0.2",
    "rc-slider": "^8.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-fontawesome": "^1.6.1",
    "sass-loader": "^6.0.6"
  }
}
